@RestController
@SpringBootApplication
public class JenkinDockerDemoApplication {

	public static void main(String[] args) {
		SpringApplication.run(JenkinDockerDemoApplication.class, args);
	}
	
	@GetMapping
	public String welcome() {
		
		return "welcome to cicd pipeleine";
	}
    
}
